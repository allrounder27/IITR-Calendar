<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.iitrtimetable"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="26"
        android:targetSdkVersion="29" />

    <application
        android:allowBackup="true"
        android:appComponentFactory="androidx.core.app.CoreComponentFactory"
        android:debuggable="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:testOnly="true"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.example.iitrtimetable.Monday"
            android:label="Monday"
            android:parentActivityName="com.example.iitrtimetable.Timetable" >
            <meta-data
                android:name="android.support.TIMETABLE"
                android:value=".ui.login.Monday" />
        </activity>
        <activity
            android:name="com.example.iitrtimetable.Timetable"
            android:label="Timetable"
            android:parentActivityName="com.example.iitrtimetable.SecondActivity" >
            <meta-data
                android:name="android.support.SECOND_ACTIVITY"
                android:value=".ui.login.Timetable" />
        </activity>
        <activity
            android:name="com.example.iitrtimetable.SecondActivity"
            android:label="@string/activity2_name"
            android:parentActivityName="com.example.iitrtimetable.ui.login.LoginActivity" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".ui.login.LoginActivity" />
        </activity>
        <activity
            android:name="com.example.iitrtimetable.ui.login.LoginActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <provider
            android:name="androidx.lifecycle.ProcessLifecycleOwnerInitializer"
            android:authorities="com.example.iitrtimetable.lifecycle-process"
            android:exported="false"
            android:multiprocess="true" />
    </application>

</manifest>